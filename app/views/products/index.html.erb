<h1>Products#index</h1>
<p>Find me in app/views/products/index.html.erb</p>
<div class="">
  <table>
    <thead>
      <tr>
        <th>Category id</th>
        <th>Name</th>
        <th>Description</th>
        <th>Price</th>
        <th>Quantity</th>
        <th>image</th>

      </tr>
    </thead>

    <tbody>
      <% @products.each do |product| %>
        <tr>
          <td><%= product.category_id %></td>
          <td><%= product.name %></td>
          <td><%= product.description %></td>
          <td><%= product.price %></td>
          <td><%= product.quantity %></td>
          <td><%= product.image %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
  <br>
  <%= link_to 'new product', new_product_path %><br>
  <%= link_to 'home', root_path %>
</div>
<div class="row">
  <div class="col-sm-6">
    <h3>All Products</h3>
    <ul>
      <% @products.each do |product| %>
      <li class="li-underline"><%= link_to product.name, product_path(product) %></li>
      <% end %>
    </ul>
  </div>
</div>
